{
  "vocabulary": {
    "directory_path": "data/vocabulary"
  },
  "dataset_reader": {
    "type": "multi-task",
    "dataset_extension": "val.aug.mrp",
    "filter_by_length": 100,
    "dataset_readers": {
      "ucca": {
        "type": "ucca_reader_conll2019",
        "token_indexers": {
          "tokens": {
            "type": "bert-pretrained",
            "pretrained_model": "bert-large-cased",
            "do_lowercase": false
          }
        },
        "action_indexers": {
          "actions": {
            "type": "single_id",
            "namespace": "ucca_actions"
          }
        },
        "arc_tag_indexers": {
          "arc_tags": {
            "type": "single_id",
            "namespace": "ucca_arc_tags"
          }
        }
      },
      "eds": {
        "type": "eds_reader_conll2019",
        "token_indexers": {
          "tokens": {
            "type": "bert-pretrained",
            "pretrained_model": "bert-large-cased",
            "do_lowercase": false
          }
        },
        "lemma_indexers": {
          "lemmas": {
            "type": "single_id",
            "namespace": "lemmas"
          }
        },
        "action_indexers": {
          "actions": {
            "type": "single_id",
            "namespace": "eds_actions"
          }
        },
        "arc_tag_indexers": {
          "arc_tags": {
            "type": "single_id",
            "namespace": "eds_arc_tags"
          }
        }
      }
    }
  },
  "validation_dataset_reader": {
    "type": "multi-task",
    "dataset_extension": "val.aug.mrp",
    "filter_by_length": 200,
    "dataset_readers": {
      "ucca": {
        "type": "ucca_reader_conll2019",
        "token_indexers": {
          "tokens": {
            "type": "bert-pretrained",
            "pretrained_model": "bert-large-cased",
            "do_lowercase": false
          }
        },
        "action_indexers": {
          "actions": {
            "type": "single_id",
            "namespace": "ucca_actions"
          }
        },
        "arc_tag_indexers": {
          "arc_tags": {
            "type": "single_id",
            "namespace": "ucca_arc_tags"
          }
        }
      },
      "eds": {
        "type": "eds_reader_conll2019",
        "token_indexers": {
          "tokens": {
            "type": "bert-pretrained",
            "pretrained_model": "bert-large-cased",
            "do_lowercase": false
          }
        },
        "lemma_indexers": {
          "lemmas": {
            "type": "single_id",
            "namespace": "lemmas"
          }
        },
        "action_indexers": {
          "actions": {
            "type": "single_id",
            "namespace": "eds_actions"
          }
        },
        "arc_tag_indexers": {
          "arc_tags": {
            "type": "single_id",
            "namespace": "eds_arc_tags"
          }
        }
      }
    }
  },
  "train_data_path": "data/full_data_for_multitask/",
  "validation_data_path": "data/full_data_for_multitask/",
  "model": {
    "type": "multi-task",
    "shared_text_field_embedder": {
      "tokens": {
        "type": "bert-pretrained",
        "pretrained_model": "bert-large-cased",
        "requires_grad": false,
        "top_layer_only": false
      },
      "embedder_to_indexer_map": {
        "tokens": ["tokens", "tokens-offsets", "tokens-type-ids"]
      },
      "allow_unmatched_keys": true
    },
    "shared_encoder":{
      "type": "stacked_self_attention",
      "input_dim": 1024,
      "hidden_dim": 768,
      "projection_dim": 512,
      "feedforward_hidden_dim": 512,
      "num_layers": 3,
      "num_attention_heads": 8,
      "use_positional_encoding": false
    },
    "task_1_encoder": {
      "type": "stacked_self_attention",
      "input_dim": 768,
      "hidden_dim": 768,
      "projection_dim": 512,
      "feedforward_hidden_dim": 512,
      "num_layers": 3,
      "num_attention_heads": 8,
      "use_positional_encoding": false
    },
    "task_2_encoder": {
      "type": "stacked_self_attention",
      "input_dim": 768,
      "hidden_dim": 768,
      "projection_dim": 512,
      "feedforward_hidden_dim": 512,
      "num_layers": 3,
      "num_attention_heads": 8,
      "use_positional_encoding": false
    },
    "models": {
      "ucca": {
        "type": "transition_parser_ucca_multi_task",
        "actions_namespace": "ucca_actions",
        "text_field_embedder": {
          "tokens": {
            "type": "bert-pretrained",
            "pretrained_model": "bert-large-cased",
            "requires_grad": false,
            "top_layer_only": false
          },
          "embedder_to_indexer_map": {
            "tokens": [
              "tokens",
              "tokens-offsets",
              "tokens-type-ids"
            ]
          },
          "allow_unmatched_keys": true
        },
        "mces_metric": {
          "type": "mces",
          "output_type": "f",
          "trace": 0,
          "cores": 10
        },
        "action_embedding": {
          "embedding_dim": 50,
          "vocab_namespace": "ucca_actions"
        },
        "word_dim": 768,
        "hidden_dim": 200,
        "action_dim": 50,
        "ratio_dim": 1,
        "num_layers": 2,
        "recurrent_dropout_probability": 0.2,
        "layer_dropout_probability": 0.2,
        "same_dropout_mask_per_instance": true,
        "input_dropout": 0.2,
        "initializer": [
          [
            "p_.*weight",
            {
              "type": "xavier_uniform"
            }
          ],
          [
            "p_.*bias",
            {
              "type": "zero"
            }
          ],
          [
            "pempty_buffer_emb",
            {
              "type": "normal"
            }
          ],
          [
            "proot_stack_emb",
            {
              "type": "normal"
            }
          ],
          [
            "pempty_action_emb",
            {
              "type": "normal"
            }
          ],
          [
            "pempty_stack_emb",
            {
              "type": "normal"
            }
          ]
        ]
      },
      "eds": {
        "type": "transition_parser_eds_multi_task",
        "actions_namespace": "eds_actions",
        "concept_label_namespace": "eds_concept_label",
        "text_field_embedder": {
          "tokens": {
            "type": "bert-pretrained",
            "pretrained_model": "bert-large-cased",
            "requires_grad": false,
            "top_layer_only": false
          },
          "embedder_to_indexer_map": {
            "tokens": [
              "tokens",
              "tokens-offsets",
              "tokens-type-ids"
            ]
          },
          "allow_unmatched_keys": true
        },
        "mces_metric": {
          "type": "mces",
          "output_type": "f",
          "trace": 0,
          "cores": 10
        },
        "lemma_text_field_embedder": {
          "lemmas": {
            "type": "embedding",
            "vocab_namespace": "lemmas",
            "embedding_dim": 25,
            "trainable": false
          }
        },
        "pos_tag_embedding": {
          "embedding_dim": 25,
          "vocab_namespace": "pos"
        },
        "action_embedding": {
          "embedding_dim": 50,
          "vocab_namespace": "eds_actions"
        },
        "concept_label_embedding": {
          "embedding_dim": 50,
          "vocab_namespace": "eds_concept_label"
        },
        "word_dim": 768,
        "hidden_dim": 200,
        "action_dim": 50,
        "concept_label_dim": 50,
        "num_layers": 2,
        "recurrent_dropout_probability": 0.2,
        "layer_dropout_probability": 0.2,
        "same_dropout_mask_per_instance": true,
        "input_dropout": 0.2,
        "initializer": [
          [
            "p_.*weight",
            {
              "type": "xavier_uniform"
            }
          ],
          [
            "p_.*bias",
            {
              "type": "zero"
            }
          ],
          [
            "pempty_buffer_emb",
            {
              "type": "normal"
            }
          ],
          [
            "proot_stack_emb",
            {
              "type": "normal"
            }
          ],
          [
            "pempty_action_emb",
            {
              "type": "normal"
            }
          ],
          [
            "pempty_stack_emb",
            {
              "type": "normal"
            }
          ],
          [
            "pempty_deque_emb",
            {
              "type": "normal"
            }
          ]
        ]
      }
    }
  },
  "iterator": {
    "type": "multi-task",
    "batch_size": 4,
    "shuffle": true
  },
  "validation_iterator": {
    "type": "multi-task",
    "batch_size": 128,
    "shuffle": false
  },
  "trainer": {
    "num_epochs": 50,
    "grad_norm": 5.0,
    "grad_clipping": 5.0,
    "patience": 50,
    "cuda_device":0,
    "validation_metric": "+average_all-f",
    "optimizer": {
      "type": "adam",
      "parameter_groups": [
        [[".*bert.*"], {"lr": 5e-5}],
        [["^((?!bert).)*$"], {}]
      ],
      "betas": [0.9, 0.999],
      "lr": 1e-3
    },
    "learning_rate_scheduler": {
      "type": "slanted_triangular",
      "num_epochs": 50,
      "num_steps_per_epoch": 1000,
      "cut_frac": 0.1,
      "ratio": 32,
      "gradual_unfreezing": true,
      "discriminative_fine_tuning": true,
      "decay_factor": 1.0
    },
    "num_serialized_models_to_keep": 1
  }
}